<Tab text="Packages" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="net.thechunk.playpen.visual.controller.PackagesTabController">
  <content>
    <AnchorPane id="Content" minHeight="300.0" minWidth="300.0" prefHeight="600.0" prefWidth="500.0">
      <children>
        <VBox prefHeight="600.0" prefWidth="500.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
          <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Packages">
              <font>
                <Font name="System Bold" size="16.0" />
              </font>
            </Text>
            <Button mnemonicParsing="false" onAction="#handleRefreshButtonPressed" text="Refresh" />
            <TreeView fx:id="packageTree" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
            <HBox alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0" spacing="10.0">
              <children>
                <Button mnemonicParsing="false" text="Promote" />
                <Button mnemonicParsing="false" onAction="#handleProvisionButtonPressed" text="Provision" />
              </children>
              <padding>
                <Insets />
              </padding>
            </HBox>
          </children>
          <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
        </VBox>
      </children>
    </AnchorPane>
  </content>
</Tab>
